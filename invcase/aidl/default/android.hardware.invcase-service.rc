service android.hardware.invcase-service /vendor/bin/hw/android.hardware.invcase-service
        interface aidl android.hardware.invcase.IInvcase/default
        class hal
        user system
        group system

# Ensure invcase-service starts only after logd is running
on property:init.svc.logd=running
    start android.hardware.invcase-service

# Ensure invcase-service starts after boot completion
on property:sys.boot_completed=1
    start android.hardware.invcase-service
        
